 @model IEnumerable<About>

<table class="table table-hover  table-striped">
    <thead>
        <tr>
            <td>Id</td>
            <td>Title</td>
            <td>Description</td>
            <td>Icon</td>
            <td>Icon Title</td>
            <td>Description</td>
            <td>Update</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.Icon</td>
                <td>@item.IconTitle</td>
                <td>@item.IconDescription</td>
                <td><a asp-action="Update" asp-controller="About" asp-route-id="@item.Id" class="btn btn-sm btn-warning">Update</a></td>
                <td><a asp-action="Delete" asp-controller="About" asp-route-id="@item.Id" class="btn btn-sm btn-danger">Delete</a></td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Index" asp-controller="About" class="btn btn-outline-primary">Back to About</a>

