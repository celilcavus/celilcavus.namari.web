@model IEnumerable<Clients>

<table class="table table-hover table-striped">
    <thead>
        <tr>
            <td>ID</td>
            <td>Title</td>
            <td>Update</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Title</td>
                <td><a asp-action="Update" asp-controller="Clients" asp-route-id="@item.Id" class="btn btn-sm btn-warning">Update</a></td>
                <td><a asp-action="Delete" asp-controller="Clients" asp-route-id="@item.Id" class="btn btn-sm btn-danger">Delete</a></td>
            </tr>
        }
    </tbody>
</table>

<br />
<a asp-action="Index" asp-controller="Clients" class="btn btn-outline-primary">Go to Back</a>